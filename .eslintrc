{
    "parser": "babel-eslint",
    "extends": "airbnb",
    "rules": {
        "func-names": 0,
        "indent": [2,4],
        "no-mixed-operators": 0,
        "no-plusplus": 0,
        "no-underscore-dangle": 0,
        "no-param-reassign": 0,
        "no-continue": 0,
        "no-script-url": 0,
        "no-control-regex": 0,
        "max-len": [2, 250],
        "prefer-rest-params": 0,
        "prefer-destructuring": 0,
        "linebreak-style": 0,
        "import/no-mutable-exports": 0,
        "import/no-extraneous-dependencies": 0,
        "import/prefer-default-export": 0,
        "jsx-a11y/href-no-hash": "off",
        "jsx-a11y/media-has-caption": "off",
        "jsx-a11y/anchor-is-valid": 0,
        "react/react-in-jsx-scope": 0,
        "react/jsx-indent": 0,
        "react/jsx-indent-props": 0,
        "react/prop-types": 0,
        "react/no-danger": 0
    },
    "env": {
        "es6": true,
        "browser": true,
        "node": true,
        "jquery": true
    },
    "parserOptions": {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures": {
            "jsx": true
        }
    },
    "plugins": [
        "react"
    ],
    "globals": {
        "wp": true,
        "_": true,
        "vc": true,
        "tinymce": true,
        "objectFitImages": true
    }
}
